<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- AutoWiring Configurations: ByName -->

	<!-- Controller Class -->
	<bean id="flipkart"
		class="org.kol.ByNameAutoWiringXmlConfiguration.FlipkartController"
		autowire="byName">

	</bean>

	<!-- Dependency -->

	<bean id="electronics"
		class="org.kol.ByNameAutoWiringXmlConfiguration.Electronics"></bean>
	<bean id="furniture"
		class="org.kol.ByNameAutoWiringXmlConfiguration.Furniture"></bean>
	<bean id="cosmetics"
		class="org.kol.ByNameAutoWiringXmlConfiguration.Cosmetics"></bean>

	<!-- AutoWiring Configurations: ByType -->

	<!-- Controller Class -->
	<bean id="amazon"
		class="org.kol.ByTypeAutoWiringXmlConfiguration.AmazonController"
		autowire="byType">


	</bean>

	<!-- Dependency -->

	<bean id="any1"
		class="org.kol.ByTypeAutoWiringXmlConfiguration.Electronics"></bean>
	<bean id="any2"
		class="org.kol.ByTypeAutoWiringXmlConfiguration.Furniture"></bean>
	<bean id="any3"
		class="org.kol.ByTypeAutoWiringXmlConfiguration.Cosmetics"></bean>



</beans>